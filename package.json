{
  "name": "babel-plugin-transform-ensure-ignore",
  "version": "0.1.0",
  "description": "require.ensure ignore",
  "main": "lib/index.js",
  "files": [
    "src/**",
    "lib/**"
  ],
  "keywords": [
    "babel-plugin"
  ],
  "config": {
    "MOCHA_OPTS": "-R dot --compilers js:babel-core/register src/__tests__/*.spec.js"
  },
  "scripts": {
    "coverage": "babel-node ./node_modules/.bin/istanbul cover _mocha -- $npm_package_config_MOCHA_OPTS",
    "ut": "mocha $npm_package_config_MOCHA_OPTS",
    "test": "npm run lint && npm run coverage",
    "build": "rm -rf lib && babel src/ --ignore __*__ --out-dir lib/",
    "release": "npm run build && npm publish",
    "lint": "eslint src/"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.3.4"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morlay/babel-plugin-transform-ensure-ignore.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/morlay/babel-plugin-transform-ensure-ignore/issues"
  },
  "homepage": "https://github.com/morlay/babel-plugin-transform-ensure-ignore#readme"
}
